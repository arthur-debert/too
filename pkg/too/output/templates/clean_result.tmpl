{{- else -}}
<success>Removed {{.RemovedCount}} finished todo(s)</success>
{{- range .RemovedTodos }}
  - <subdued>#{{.Position}}</subdued>: {{.Text}}
{{- end }}
{{- end }}
<count>{{.ActiveCount}} active todo(s) remaining</count>