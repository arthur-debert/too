{{- if eq .RemovedCount 0 -}}
<warning>No finished todos to clean</warning>
{{- else -}}
<success>Removed {{.RemovedCount}} finished todo(s)</success>
{{- end -}}
{{- if .ActiveTodos }}

{{renderNestedTodos .ActiveTodos "" 0 -}}
<count>{{.ActiveCount}} todo(s), 0 done</count>
{{- else }}

<warning>No todos remaining</warning>
{{- end -}}